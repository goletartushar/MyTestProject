<script>
    const filterButtons = document.querySelectorAll('.properties-filter a');
const cards = document.querySelectorAll('.properties .col-lg-4');

filterButtons.forEach(button => {
  button.addEventListener('click', function (e) {
    e.preventDefault();

    // Remove active class from all buttons
    filterButtons.forEach(btn => btn.classList.remove('is_active'));

    // Add active class to clicked button
    this.classList.add('is_active');

    const filter = this.getAttribute('data-filter');

    cards.forEach(card => {
      const categories = card.getAttribute('data-category')?.toLowerCase() || '';

      if (filter === '*' || categories.includes(filter.toLowerCase())) {
        card.style.display = 'block';
      } else {
        card.style.display = 'none';
      }
    });
  });
});

</script>
